{
	"presets": [
		"@babel/preset-env",
		"@babel/preset-react"
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		["babel-plugin-root-import",
            {
                "paths": [{
                        "rootPathPrefix": "~",
                        "rootPathSuffix": "src"
                    }
                ]
            }
        ],
	]
}
